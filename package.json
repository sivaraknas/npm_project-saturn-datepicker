    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.24.0",
    "ng-packagr": "^12.2.7",
    "protractor": "^7.0.0",
    "rimraf": "^2.7.1",
    "ts-node": "~7.0.0",
    "tsickle": "0.37.0",
